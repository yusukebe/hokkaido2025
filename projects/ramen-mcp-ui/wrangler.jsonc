{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "ramen-mcp-ui",
  "compatibility_date": "2025-08-03",
  "main": "./src/server/index.tsx",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "migrations": [
    {
      "new_sqlite_classes": [
        "MyMCP"
      ],
      "tag": "v1"
    }
  ],
  "durable_objects": {
    "bindings": [
      {
        "class_name": "MyMCP",
        "name": "MCP_OBJECT"
      }
    ]
  }
}